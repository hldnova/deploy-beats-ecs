# sample inventory
[filebeat:children]
vdc1
vdc2

[metricbeat:children]
vdc1
vdc2

[ecsbeat:children]
vdc1-ecsbeat
vdc2-ecsbeat

[syslog:children]
vdc1
vdc2

[vdc1]
10.249.249.192

[vdc1:vars]
ecs_vdc_name=vdc1
ecs_vdc_id=vdc1_1234

[vdc1-ecsbeat]

[vdc1-ecsbeat:vars]
ecs_hosts=10.249.249.192:4443
ecs_username=root
ecs_password=ChangeMe

[vdc2]

[vdc2:vars]
ecs_vdc_name=vdc2
ecs_vdc_id=vdc2_1234

[vdc2-ecsbeat]

[vdc2-ecsbeat:vars]
ecs_hosts=10.1.21.205:4443
ecs_username=user
ecs_password=pass
